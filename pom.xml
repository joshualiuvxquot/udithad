        <org.wso2.carbon.version>4.4.7</org.wso2.carbon.version>
        <synapse.version>2.1.7-wso2v3</synapse.version>
        <carbon.apimgt.version>6.0.4</carbon.apimgt.version>
        <com.h2database.wso2.version>1.2.140.wso2v3</com.h2database.wso2.version>
