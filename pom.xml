        <org.glassfish.jersey.core.version>2.4</org.glassfish.jersey.core.version>
        <com.wso2telco.core.version>2.0.3-M4</com.wso2telco.core.version>
        <org.json.wso2.version>3.0.0.wso2v1</org.json.wso2.version>
        <junit.version>3.8.1</junit.version>
        <org.apache.synapse.core.version>2.1.7-wso2v3</org.apache.synapse.core.version>
        <org.wso2.carbon.commons.version>4.4.8</org.wso2.carbon.commons.version>
        <org.wso2.carbon.apimgt.version>6.0.4</org.wso2.carbon.apimgt.version>
        <org.wso2.hazelcast>3.5.2.wso2v1</org.wso2.hazelcast>
        <rhino.wso2.version>1.7.0.R4.wso2v1</rhino.wso2.version>
        <org.jaggeryjs.version>0.12.6</org.jaggeryjs.version>
        <commons-io.wso2.version>2.4.0.wso2v1</commons-io.wso2.version>
