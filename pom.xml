<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>com.wso2telco.dep</groupId>
        <artifactId>component-dep-parent</artifactId>
        <version>2.0.0-SNAPSHOT</version>
    </parent>

    <groupId>com.wso2telco.dep</groupId>
    <artifactId>component-dep</artifactId>
    <packaging>pom</packaging>
    <version>2.0.0-SNAPSHOT</version>
    <name>WSO2Telco -Digital Enable Platform Component</name>
    <description>WSO2Telco DEP Aggregate Pom</description>
    <url>http://wso2telco.com</url>
    <developers>
        <developer>
            <name>WSO2Telco</name>
            <email>core-product-tech@wso2telco.com</email>
        </developer>
    </developers>

    <prerequisites>
        <maven>3.0.0</maven>
    </prerequisites>

    <distributionManagement>
        <snapshotRepository>
            <id>wso2Telco.snapshots</id>
            <name>Snapshot repo</name>
            <url>http://maven.wso2telco.com/nexus/content/repositories/snapshots</url>
        </snapshotRepository>
        <!-- Publish the versioned releases here -->
        <repository>
            <id>wso2telco.releases</id>
            <name>release repo</name>
            <url>http://maven.wso2telco.com/nexus/content/repositories/releases/</url>
        </repository>
    </distributionManagement>


    <modules>
        <module>parent</module>
        <module>components/oneapi-validation</module>
        <module>components/subscription-validator</module>
        <!--module>components/publish-events-data</module-->
        <!--module>components/data-publisher</module-->
        <module>components/reporting-service</module>
        <module>components/verification-handler</module>
        <module>components/operator-service</module>
        <module>components/mediator</module>
        <!--module>components/publish-events/publish-events-data</module>
          <module>components/publish-events/publish-events-extensions</module-->
        <module>workflow/workflow-approval</module>
        <module>components/workflow-extensions</module>
        <module>workflow/workflow-service</module>
        <module>components/server-startup-observer</module>
        <module>webapps/blacklist-whitelist-service</module>
        <module>webapps/aggregator-blacklist</module>
        <module>jaggery-apps/manage</module>
        <module>jaggery-apps/publisher</module>
        <module>jaggery-apps/sandbox</module>
        <module>jaggery-apps/store</module>
        <module>features</module>
    </modules>

    <scm>
        <url>https://github.com/WSO2Telco/component-dep.git</url>
        <developerConnection>scm:git:https://github.com/WSO2Telco/component-dep.git</developerConnection>
        <connection>scm:git:https://github.com/WSO2Telco/component-dep.git</connection>
        <tag>HEAD</tag>
    </scm>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.felix</groupId>
                    <artifactId>maven-scr-plugin</artifactId>
                    <version>${maven-scr-plugin.version}</version>
                    <executions>
                        <execution>
                            <id>generate-scr-scrdescriptor</id>
                            <goals>
                                <goal>scr</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.apache.felix</groupId>
                    <artifactId>maven-bundle-plugin</artifactId>
                    <version>${maven-bundle-plugin.version}</version>
                    <extensions>true</extensions>
                    <configuration>
                        <obrRepository>NONE</obrRepository>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-release-plugin</artifactId>
                    <configuration>
                        <preparationGoals>clean install</preparationGoals>
                        <autoVersionSubmodules>true</autoVersionSubmodules>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-deploy-plugin</artifactId>
                </plugin>

            </plugins>
        </pluginManagement>

    </build>


    <properties>
